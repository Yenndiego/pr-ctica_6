ejercicio2
determinar si a > b
#include <stdio.h>

int main()
{
  int a;
  int b;
printf("introduce los dos números\n");
scanf("%d",&a);
printf("introduce el segundo números\n");
scanf("%d",&b);
{
     if(a > b)
     {
         printf("el segundo número es mayor");
     }
     else
     {
           printf("el primero valor es mayor");
     }  

}
  return 0;
  }
